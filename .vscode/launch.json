{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Editor",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/src",
			"cwd": "${workspaceFolder}",
			"buildFlags": "-tags=debug,editor",
			"env": {
				"CGO_ENABLED": "1",
				// "GOEXPERIMENT": "greenteagc"
			}
		}, {
			"name": "Trace Editor",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/src",
			"cwd": "${workspaceFolder}",
			"args": ["-trace"],
			"buildFlags": "-tags=debug,editor",
			"env": {
				"CGO_ENABLED": "1",
				// "GOEXPERIMENT": "greenteagc"
			}
		}, {
			"name": "Documentation Generator",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/src/generators/api",
			"cwd": "${workspaceFolder}",
			"env": {
				"CGO_ENABLED": "1",
				// "GOEXPERIMENT": "greenteagc"
			}
		}, {
			"name": "Copy Headers",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/src/generators/copy_header",
			"cwd": "${workspaceFolder}",
			"env": {
				"CGO_ENABLED": "1",
				// "GOEXPERIMENT": "greenteagc"
			}
		}, {
			"name": "Debug Build Android",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/src/generators/engine_builds/engine_builds_android",
			"cwd": "${workspaceFolder}",
			"buildFlags": "-tags=debug,generator",
			"env": {
				"CGO_ENABLED": "1",
				// "GOEXPERIMENT": "greenteagc"
			}
		}
	]
}